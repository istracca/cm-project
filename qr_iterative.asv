function [Q,R] = qr_iterative(H, Q, R, n)
    Q(n,n) = 1;
    R(1:n-1,n-1) = Q(:,1:n-1)' * H(:,n-1);
    R(n,n-1) = H(n,n-1);
    c = H(n-1,n-1)/sqrt(H(n-1,n-1)^2 + H(n,n-1)^2)
    s = H(n,n-1)/sqrt(H(n-1,n-1)^2 + H(n,n-1)^2)
    Rot = eye(n);
    Rot(n-1,n-1) = c;
    Rot(n-1,n) = s;
    Rot(n,n-1) = -s;
    Rot(n,n) = c;
    Rot'*R
end